type: go
name: auth-api
host: 0.0.0.0
port: 8888

service authpb.AuthService

routes:
  - method: POST
    path: /api/auth/guest-login
    handler: GuestLogin
  - method: POST
    path: /api/auth/password-login
    handler: PasswordLogin
  - method: POST
    path: /api/auth/phone-login
    handler: PhoneLogin
  - method: POST
    path: /api/auth/oauth-login
    handler: OAuthLogin
  - method: POST
    path: /api/auth/logout
    handler: Logout
